{
  "kind": "collectionType",
  "collectionName": "best_matches",
  "info": {
    "singularName": "best-match",
    "pluralName": "best-matches",
    "displayName": "Best Match"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Company": {
      "type": "string",
      "required": true
    },
    "JobTitle": {
      "type": "string",
      "required": true
    },
    "TimeZone": {
      "type": "string",
      "required": true
    },
    "certifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::certification.certification",
      "mappedBy": "best_match"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other"
      ],
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true
    }
  }
}
